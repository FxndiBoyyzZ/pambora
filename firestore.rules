rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: This is a temporary rule for launch.
    // It makes your database completely public.
    // ANYONE can read, write, and delete your data.
    // You MUST secure this before going into full production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
